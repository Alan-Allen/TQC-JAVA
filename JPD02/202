import java.util.Scanner;

public class JPD02 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		try {
			int a = sc.nextInt();
			if(a % 3 == 0) {
				System.out.print(a + " is an odd number.");
			} else {
				System.out.print(a  + " is an even number.");
			}
		} catch (Exception e) {
			System.out.println("error");
            return;
		}
	}
}